{
  "compilerOptions": {
    "baseUrl": "./",
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist/",
    "rootDirs": ["src","test"],
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": true,
    "target": "es2021",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src","test"],
}